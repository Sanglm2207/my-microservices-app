{
  "name": "auth-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc --build",
    "db:migrate": "dotenv -e .env -- prisma migrate dev --schema=../../packages/database/prisma/schema.prisma",
    "db:generate": "dotenv -e .env -- prisma generate --schema=../../packages/database/prisma/schema.prisma",
    "db:studio": "dotenv -e .env -- prisma studio --schema=../../packages/database/prisma/schema.prisma"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cache": "workspace:*",
    "common-types": "workspace:*",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "database": "workspace:*",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "validation": "workspace:^",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "dotenv-cli": "^10.0.0",
    "prisma": "^5.7.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig": "workspace:*",
    "typescript": "^5.3.3"
  }
}